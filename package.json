    "webpack-dev-server": "^2.1.0-beta",
    "webpack-merge": "^0.14"
  },
  "scripts": {
    "start": "node config/webpack.server.js",
    "build": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "build:pages": "NODE_ENV=production webpack --config config/webpack.gh-pages.js",
    "analyze": "NODE_ENV=production webpack --config config/webpack.prod.js --profile --json > webpack.stats.json",
    "lint": "eslint  --ext .js --ext .jsx app/scripts test",
    "lint:styles": "stylelint app/styles/**/*.scss",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ui": "nightwatch -c test/lib/nightwatch.conf.js",
    "test:ui:start": "npm start -- test:ui",
    "deploy": "node config/tools deploy",
    "publish": "node config/tools publish",
    "docs": "node config/tools docs",
    "precommit": "node config/tools commits && npm run lint && npm test",
    "postmerge": "node config/tools update && npm update"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "app/scripts"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/test/lib/styleMock.js",
      "^.+\\.(jpe?g|png|gif|ttf|eot|svg|md)$": "<rootDir>/test/lib/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/test/lib/setup.js"
    ],
    "testRegex": "/test/.*?\\.(test|spec)\\.js$",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "app/scripts/**/*.{js,jsx}",
      "!app/scripts/**/*.dev.{js,jsx}",
      "!app/scripts/**/DevTools.jsx",
      "!app/scripts/vendor/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 80,
        "lines": 90,
        "statements": 90
      }
    },
    "verbose": true
  }
}
