    "ease-component": "^1.0",
    "fbjs": "^0.8",
    "history": "^4.5",
    "lodash": "^4.17",
    "mobile-detect": "^1.3",
    "modernizr": "^3.3",
    "moment": "^2.17",
    "react": "^15.4",
    "react-dom": "^15.4",
    "react-history": "^0.18",
    "react-inlinesvg": "^0.5",
    "react-redux": "^5.0",
    "react-router-dom": "^4.0.0-beta",
    "redux": "^3.6",
    "redux-observable": "^0.13",
    "redux-persist": "^4.3",
    "redux-thunk": "^2.2",
    "rxjs": "^5.1",
    "sass-mediaqueries": "^1.6",
    "scroll": "^2.0",
    "scroll-doc": "^0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7",
    "babel-core": "^6.22",
    "babel-eslint": "^7.1",
    "babel-jest": "^18.0",
    "babel-loader": "^6.2",
    "babel-plugin-istanbul": "^3.1",
    "babel-plugin-lodash": "^3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3",
    "babel-plugin-transform-object-assign": "^6.22",
    "babel-plugin-transform-runtime": "^6.22",
    "babel-preset-latest": "^6.22",
    "babel-preset-react": "^6.22",
    "babel-preset-stage-1": "^6.22",
    "babel-register": "^6.22",
    "babel-runtime": "^6.22",
    "browser-sync": "^2.18",
    "browser-sync-webpack-plugin": "^1.1",
    "chalk": "^1.1",
    "chromedriver": "^2.27",
    "clean-webpack-plugin": "^0.1",
    "copy-webpack-plugin": "^4.0",
    "css-loader": "^0.26",
    "del": "^2.2",
    "enzyme": "^2.7",
    "esdoc": "^0.5",
    "esdoc-es7-plugin": "^0.0.3",
    "eslint": "^3.15",
    "eslint-config-airbnb": "^14.1",
    "eslint-import-resolver-webpack": "^0.8",
    "eslint-loader": "^1.6",
    "eslint-plugin-babel": "^4.0",
    "eslint-plugin-flowtype": "^2.30",
    "eslint-plugin-import": "^2.2",
    "eslint-plugin-jsx-a11y": "^4.0",
    "eslint-plugin-react": "^6.9",
    "exports-loader": "^0.6",
    "expose-loader": "^0.7",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "fetch-mock": "^5.9",
    "file-loader": "^0.10",
    "html-loader": "^0.4",
    "html-webpack-plugin": "^2.28",
    "husky": "^0.13",
    "image-webpack-loader": "^3.2",
    "imports-loader": "^0.7",
    "jest-cli": "^18.1",
    "jsdom": "^9.10",
    "json-loader": "^0.5",
    "markdown-loader": "^2.0",
    "modernizr-loader": "^1.0",
    "nightwatch": "^0.9",
    "node-sass": "^4.5",
    "null-loader": "^0.1",
    "offline-plugin": "^4.6",
    "postcss-loader": "^1.2",
    "raw-loader": "^0.5",
    "react-addons-test-utils": "^15.4",
    "react-hot-loader": "^3.0.0-beta",
    "reactotron-react-js": "^1.7",
    "reactotron-redux": "^1.7",
    "redux-diff-logger": "^0.0.9",
    "redux-logger": "^2.8",
    "redux-mock-store": "^1.2",
    "rsync": "^0.6",
    "sass-loader": "^5.0",
    "script-loader": "^0.7",
    "selenium-server-standalone-jar": "^3.0",
    "source-map-support": "^0.4",
    "style-loader": "^0.13",
    "stylelint": "^7.8",
    "stylelint-config-standard": "^16.0",
    "stylelint-order": "^0.2",
    "stylelint-scss": "^1.4",
    "url-loader": "^0.5",
    "webpack": "^2.2",
    "webpack-dev-server": "^2.3",
    "webpack-merge": "^2.6",
    "xhr-mock": "^1.7"
  },
  "scripts": {
    "start": "node config/webpack.server.js",
    "build": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "analyze": "NODE_ENV=production webpack --config config/webpack.prod.js --profile --json > webpack.stats.json",
    "lint": "eslint  --ext .js --ext .jsx app/scripts config test",
    "lint:styles": "stylelint app/styles/**/*.scss",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ui": "nightwatch -c test/lib/nightwatch.conf.js",
    "test:ui:start": "npm start -- test:ui",
    "deploy": "node config/tools deploy",
    "publish": "node config/tools publish",
    "docs": "node config/tools docs",
    "precommit": "node config/tools commits && npm run lint && npm run lint:styles && npm test",
    "postmerge": "node config/tools update && npm update"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "jest": {
    "transform": { ".*": "<rootDir>/node_modules/babel-jest" },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "app/scripts"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/test/__setup__/styleMock.js",
      "^.+\\.(jpe?g|png|gif|ttf|eot|svg|md)$": "<rootDir>/test/__setup__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/test/__setup__/index.js"
    ],
    "testRegex": "/test/.*?\\.(test|spec)\\.js$",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "app/scripts/**/*.{js,jsx}",
      "!app/scripts/**/*.dev.{js,jsx}",
      "!app/scripts/**/DevTools.jsx",
      "!app/scripts/vendor/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "verbose": true
  }
}
