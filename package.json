{
  "name": "react-redux-observables-boilerplate",
  "title": "React-Redux-Observables Boilerplate",
  "description": "Yet another starter kit for React with Router, Redux, Observable + RxJS",
  "version": "0.3.0",
  "author": "Gil Barbara <gilbarbara@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/gilbarbara/react-redux-observables-boilerplate"
  },
  "bugs": "https://github.com/gilbarbara/react-redux-observables-boilerplate/issues",
  "license": "MIT",
  "keywords": [
    "React",
    "React-Router",
    "Redux",
    "Redux-Observable",
    "RxJS",
    "ReactiveX",
    "Webpack",
    "ES6",
    "ES7"
  ],
  "dependencies": {
    "bootstrap": "^4.0.0-alpha",
